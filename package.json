{
  "name": "@prostory/mountain",
  "version": "0.5.0",
  "description": "Yet another server and client. For creating HTTP/2 servers and clients.",
  "type": "module",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/YevhenKap/mountain"
  },
  "scripts": {
    "clean": "rm -r build/*",
    "copy:declaration": "cp src/declaration/index.d.ts build/index.d.ts",
    "copy:docs": "cp README.md build/README.md",
    "copy:changelog": "cp CHANGELOG.md build/CHANGELOG.md",
    "copy:license": "cp LICENSE build/LICENSE",
    "copy:package": "cp package.json build/package.json",
    "copy": "npm run copy:declaration; npm run copy:docs; npm run copy:changelog; npm run copy:license; npm run copy:package",
    "prebuild": "npm run copy",
    "build": "rollup -c",
    "prerelease": "npm run build",
    "release": "npm publish --access public"
  },
  "keywords": [
    "Server",
    "Client",
    "HTTP/2"
  ],
  "author": "Kapelianovych Yevhen",
  "license": "MIT",
  "dependencies": {
    "@fluss/core": "^0.8.0",
    "mime": "^2.4.6"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.5",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}
